<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Component" table="component">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userid" type="integer" column="userid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="comment" type="string" column="comment" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="blscode" type="string" column="blscode" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="fatType" type="smallint" column="fat_type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
