<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Shape" table="shape">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="integer" column="code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sym1" type="boolean" column="SYM1" nullable="true"/>
    <field name="sym2" type="boolean" column="SYM2" nullable="true"/>
    <field name="sym3" type="boolean" column="SYM3" nullable="true"/>
    <field name="axisym1" type="boolean" column="AXISYM1" nullable="true"/>
    <field name="axisym2" type="boolean" column="AXISYM2" nullable="true"/>
    <field name="axisym3" type="boolean" column="AXISYM3" nullable="true"/>
  </entity>
</doctrine-mapping>
